/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "sourceMap": true,
    "types": [],
    "paths": {
      "@docs-components/*": ["./src/components/*"],
      "@app/*": ["src/app/*"],
      "@environments/*": ["src/environments/*"],
      "@model/*": ["src/app/object/*"],
      "@asset/*": ["src/assets/*"]
    }
  },
  "files": [
    "src/main.ts",
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
